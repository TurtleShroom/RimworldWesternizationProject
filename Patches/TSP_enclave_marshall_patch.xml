<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<!-- If you are not American, the Enclave hates you. Marshalls are American. -->
	<Operation Class="PatchOperationConditional">
	<!-- Does this exist? -->
		<xpath>/Defs/FactionDef[defName="DRK_Enclave"]</xpath>
		<!-- If it does, add this. -->
		<match Class="PatchOperationAdd">
			<xpath>/Defs/FactionDef[defName="DRK_Enclave"]/permanentEnemyToEveryoneExcept</xpath>
			<value>
				<!-- <li>RWP_Faction_Marshal_Core</li> -->
				<li>RWP_Faction_Marshal</li>
			</value>
		</match>
	</Operation>
</Patch>