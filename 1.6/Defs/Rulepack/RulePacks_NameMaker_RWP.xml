<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <!-- ==================== RWP Marshal Faction ================= -->
  <RulePackDef>
    <defName>NamerSettlementMarshal</defName>
    <rulePack>
      <rulesStrings>
        <li>name->[Color] [TerrainFeature] Marshal Outpost</li>
        <li>name->[Animal] [TerrainFeature] Marshal Outpost</li>
        <li>name->[topicalnoun] [TerrainFeature] Marshal Outpost</li>
        <li>name->[topicalnoun]'s [TerrainFeature] Marshal Outpost</li>
        <li>name->[concept_positive]'s [TerrainFeature] Marshal Outpost</li>
        <li>name->[concept_positive] [TerrainFeature] Marshal Outpost</li>
        <li>name->[topicalnoun] [TerrainFeature] Marshal Outpost</li>
        <li>name->[topicalnoun]'s [TerrainFeature] Marshal Outpost</li>
        <li>name->[Color] [geography] Marshal Outpost</li>
        <li>name->[Animal] [geography] Marshal Outpost</li>
        <li>name->[topicalnoun] [geography] Marshal Outpost</li>
        <li>name->[topicalnoun]'s [geography] Marshal Outpost</li>
        <li>name->[concept_positive]'s [geography] Marshal Outpost</li>
        <li>name->[concept_positive] [geography] Marshal Outpost</li>
        <li>name->[topicalnoun] [geography] Marshal Outpost</li>
        <li>name->[topicalnoun]'s [geography] Marshal Outpost</li>
      </rulesStrings>
    </rulePack>
  </RulePackDef>
  <!-- ==================== RWP Bandit Faction ================= -->
  <RulePackDef>
    <defName>NamerFactionBandit</defName>
    <rulePack>
      <rulesStrings>
        <li>name->The [badassanimal] Gang</li>
        <li>name->The [badassperson] Gang</li>
        <li>name->The [weapon] Gang</li>
        <li>name->The [badasscolor] [weapon] Gang</li>
        <li>name->The [badasscolor] [badassanimal] Gang</li>
        <li>name->The [badassperson] Gang</li>
        <li>name->The [badassadjective] Gang</li>
        <li>name->The [badassconcept] Gang</li>
      </rulesStrings>
      <rulesRaw>
        <li Class="Rule_File">
          <keyword>badassanimal</keyword>
          <path>Words/Nouns/Animals_Badass</path>
        </li>
        <li Class="Rule_File">
          <keyword>badassadjective</keyword>
          <path>Words/Adjectives/Badass</path>
        </li>
        <li Class="Rule_File">
          <keyword>badassperson</keyword>
          <path>Words/Nouns/People_Badass</path>
        </li>
        <li Class="Rule_File">
          <keyword>badassconcept</keyword>
          <path>Words/Nouns/Concepts_Badass</path>
        </li>
        <li Class="Rule_File">
          <keyword>groupname</keyword>
          <path>Words/Nouns/GroupNames</path>
        </li>
        <li Class="Rule_File">
          <keyword>badasscolor</keyword>
          <path>Words/Nouns/Colors_Badass</path>
        </li>
      </rulesRaw>
    </rulePack>
  </RulePackDef>
</Defs>
