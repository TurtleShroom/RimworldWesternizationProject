<?xml version="1.0" encoding="utf-8"?>
<Patch>
  <!-- ========== Pioneer Town Base ========== -->
  <Operation Class="PatchOperationAdd">
    <xpath>Defs/TraderKindDef[defName="RWP_Base_Town_Standard"]/stockGenerators</xpath>
    <value>
      <li Class="StockGenerator_Tag">
        <tradeTag>CE_Ammo</tradeTag>
        <countRange>
          <min>500</min>
          <max>2000</max>
        </countRange>
        <thingDefCountRange>
          <min>5</min>
          <max>9</max>
        </thingDefCountRange>
      </li>
      <li Class="StockGenerator_Category">
        <categoryDef>Ammo</categoryDef>
        <thingDefCountRange>
          <min>0</min>
          <max>0</max>
        </thingDefCountRange>
      </li>
    </value>
  </Operation>
  <!-- ========== Pioneer Town Caravan ========== -->
  <!-- General Goods Trader -->
  <Operation Class="PatchOperationAdd">
    <xpath>Defs/TraderKindDef[defName="RWP_Caravan_BulkGoods"]/stockGenerators</xpath>
    <value>
      <li Class="StockGenerator_Tag">
        <tradeTag>CE_Ammo</tradeTag>
        <countRange>
          <min>200</min>
          <max>400</max>
        </countRange>
        <thingDefCountRange>
          <min>1</min>
          <max>4</max>
        </thingDefCountRange>
      </li>
      <li Class="StockGenerator_Category">
        <categoryDef>Ammo</categoryDef>
        <thingDefCountRange>
          <min>0</min>
          <max>0</max>
        </thingDefCountRange>
      </li>
    </value>
  </Operation>
  <!-- Gunsmith Trader -->
  <Operation Class="PatchOperationAdd">
    <xpath>Defs/TraderKindDef[defName="RWP_Caravan_Combat"]/stockGenerators</xpath>
    <value>
      <li Class="StockGenerator_Tag">
        <tradeTag>CE_Ammo</tradeTag>
        <countRange>
          <min>400</min>
          <max>1000</max>
        </countRange>
        <price>Cheap</price>
        <thingDefCountRange>
          <min>3</min>
          <max>8</max>
        </thingDefCountRange>
      </li>
      <li Class="StockGenerator_Category">
        <categoryDef>Ammo</categoryDef>
        <thingDefCountRange>
          <min>0</min>
          <max>0</max>
        </thingDefCountRange>
      </li>
    </value>
  </Operation>
</Patch>